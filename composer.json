	"type": "library", 
	"keywords": [
		"Utility", 
		"MIME"
	], 
	"license": "MIT", 
	"authors": [
			{
			"name": "Renaud Guillard", 
			"email": "dev@nore.fr", 
			"role": "lead"
			}
